{
  "host": "HOSTNAME",
  "port": "PORT",
  "lineAccessToken": "LINE_CHANNEL_ACCESS_TOKEN",
  "lineSecret": "LINE_CHANNEL_SECRET",
  "postgres": "DATABASE_URL",
  "salt": "ENCRYPT_SALT",
  "frontend": "FRONTEND_URL",
  "authentication": {
    "secret": "AUTH_SECRET",
    "authStrategies": [
      "jwt",
      "local",
      "oauth"
    ],
    "jwtOptions": {
      "header": {
        "typ": "access"
      },
      "audience": "JWT_AUDIANCE",
      "issuer": "JWT_ISSUERS",
      "algorithm": "HS256",
      "expiresIn": "1d"
    },
    "oauth": {
      "redirect": "OAUTH_REDIRECT_URL",
      "auth0": {
        "key": "OAUTH_CLIENT_ID",
        "secret": "OAUTH_CLIENT_SECRET",
        "subdomain": "OAUTH_SUBDOMAIN",
        "scope": [
          "openid",
          "profile",
          "email"
        ]
      }
    }
  }
}